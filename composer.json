{
    "name": "loggerv1.gci",
    "description": "GCI Central Log Server Interface",
    "keywords": ["GCI", "SWYCH", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Pawan Singh",
            "email": "pawan@giftcardsindia.in",
            "homepage": "http://loggerv1.giftcardsindia.in/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "bryanjhv/slim-session": "~3.0",
        "doctrine/orm": "^2",
        "google/apiclient": "^2.0",
        "monolog/monolog": "^1.17",
        "phpmailer/phpmailer": "^6.1",
        "slim/flash": "^0.4.0",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Gci\\": "src/Gci"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
